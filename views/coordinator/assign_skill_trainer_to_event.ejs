	
		<div class="modal-dialog">
		    <div class="modal-content">
		      <div class="modal-header">
		        <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="coordinator_event_details('hide')">&times;</button>
		        <h4 class="modal-title" id="myModalLabel">Assign</h4>
		      </div>
		      
		      <div class="modal-body" style="background: #fff">
		      	
			      	<div id="error-message">
			      	</div>
			      	
			      	<table>
			      		<tr>
			      			<td><label><b>Select Skill</b></label></td>
			      			<td style="padding: 4px">
			      				<select name="SkillId" id="SkillId" onchange="get_modules_for_skill()" class="form-control" style="border: 1px solid #ccc">
						      		<% skills.forEach(function(skill, i) { %>
						      			<option value="<%= skill.id %>"><%= skill.name %></option>
						      		<% }); %>
						      	</select>
			      			</td>
			      		</tr>
			      		<tr>
			      			<td><label><b>Select Module</b></label></td>
			      			<td id="module_div" style="padding: 4px">
			      				<select name="ModuleId" id="ModuleId" class="form-control" style="border: 1px solid #ccc">
						      		<% modules.forEach(function(module, i) { %>
						      			<option value="<%= module.id %>"><%= module.name %></option>
						      		<% }); %>
						      	</select>
			      			</td>
			      		</tr>
			      		<tr>
			      			<td><label><b>Select Trainer</b></label></td>
			      			<td style="padding: 4px">
			      				<select name="TrainerId" id="TrainerId" class="form-control" style="border: 1px solid #ccc">
						      		<% trainers.forEach(function(trainer, i) { %>
						      			<option value="<%= trainer.id %>"><%= trainer.username %></option>
						      		<% }); %>
						      	</select>
			      			</td>
			      		</tr>
			      	</table>
			      	
		      </div>
		      
		      <div class="modal-footer" style="text-align: center">
			    	<input type="hidden" name="flag" id="flag" value=""/>
			    	<a class="btn btn-success" onclick="do_assign_skill_trainer_to_event('<%= event_id %>','<%= currentUser.id %>','do not proceed')">Submit</a>
			    	<a class="btn btn-danger display-none" onclick="do_assign_skill_trainer_to_event('<%= event_id %>','<%= currentUser.id %>', 'proceed')" id="submit-with-conflict-btn">Submit with Conflict</a>
			  </div>
		      
			</div>
		</div>
	