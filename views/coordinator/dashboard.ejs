						<!-- end maps -->
						
						<!-- start alerts -->
						<div class="row">
							<div class="col-md-12">
								<div class="panel panel-primary">
								  <div class="panel-heading">
								    <div class="panel-title text-center">
								      Alerts
								    </div>
								  </div>
				
								  <div class="panel-body-collapse collapse in">
								    <div class="panel-body tab-content">
								      <div class="tab-pane active fade in">
									  	<ul class="nav nav-stacked nano-content" style="height: 150px; overflow: auto">
									        <% if(alerts.length == 0) { %>
									        	<li>No alerts</li>
									        <% } %>
									        <% alerts.forEach(function(event, j) { %>
												<li class="chat-contact unread">
												  <a class="media">
												    <img class="media-object pull-left img-circle" src="/wrap/images/faker/avatars/48/Faces_Users-22.png">
												    <div class="media-body">
												      <div class="media-heading" style="color: #43494d;">
												        <%= event.name %>&nbsp;<%= event.date %>&nbsp;,<%=event.status %>
												      </div>
												      <small class="last-message">
												        <%= event.description %>
												      </small>
												    </div>
												  </a>
												</li>
											<% }); %>
									    </ul>
									    
								      </div>
								      
								    </div>
								  </div> <!-- end panel-body-collapse -->
								</div> 
							</div>
						</div>
						<!-- end alerts -->
						
					</div>
					
					<div class="col-md-6">
						<!-- start event feed -->
						<div class="col-md-12">
							<div class="panel panel-info">
							  <div class="panel-heading">
							    <div class="panel-actions panel-reveal pull-right">
							    	<i class="icm icm-history" title="Refresh" onclick="ajax_call('/coordinator/refresh_today_events','refresh-today-events')"></i>
							    </div>
							    <div class="panel-title text-center">
							      Today's Events <%= events.length > 0 ? '('+events[0].date.toDateString()+')' : '' %>
							    </div>
							  </div>
			
							  <div class="panel-body-collapse collapse in">
							    <div class="panel-body tab-content">
							      <div class="tab-pane active fade in">
								  	<ul class="nav nav-stacked nano-content" style="height: 460px; overflow: auto" id="refresh-today-events">
								        <% if(events.length == 0) { %>
									        	<li>No events</li>
									        <% } %>
								        <% events.forEach(function(event, j) { %>
											<li class="chat-contact unread">
											  <a class="media">
											    <img class="media-object pull-left img-circle" src="/wrap/images/faker/avatars/48/Faces_Users-22.png">
											    <div class="media-body">
											      <div class="media-heading" style="color: #43494d;">
											        <%= event.name %>&nbsp;<%= event.date.getHours() %>:<%= event.date.getMinutes() %> to <%= event.endDate != null ? event.endDate.getHours() : '' %>:<%= event.endDate != null ? event.endDate.getMinutes() : '' %>,&nbsp;<%= event.org %>&nbsp;<%= event.trainer %>&nbsp;,<b><%=event.status %></b>
											      </div>
											      <small class="last-message">
											        <% if(event.status != 'Trainer Accepted Invite') { %>
											        	<btn class="btn btn-primary btn-sm turbo-card turbo-toggle" data-back-class="bg-info" data-turbo-url="/coordinator/get_trainer_location/<%= event.id %>" title="Map"><i class="icm icm-location3"></i></btn>
											        <% } %>
											        <% if(event.status == 'Done Event' || event.status == 'checkout') { %>
											        	<btn data-toggle="modal" data-target="#uide4b3dde6bb5097375d99eb18a83d7146" onclick="ajax_call('/coordinator/view_attendance/<%= event.id %>','uide4b3dde6bb5097375d99eb18a83d7146')" class="btn btn-warning btn-sm" title="Attendance">Attendance</btn>
											        	<btn data-toggle="modal" data-target="#uide4b3dde6bb5097375d99eb18a83d7146" onclick="ajax_call('/coordinator/view_trainer_feedback/<%= event.id %>','uide4b3dde6bb5097375d99eb18a83d7146')" class="btn btn-warning btn-sm" title="Feedback">Feedback</btn>
											        <% } %>
											      </small>
											    </div>
											  </a>
											</li>
										<% }); %>
								    </ul>
								    
