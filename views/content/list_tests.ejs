<input type="button" class="first-btn btn btn-primary" value="Create New Test" onclick="ajax_call('/content/new_test','content-admin-display')">

<h2 class="text-gradient-info-dk text-center">Test List</h2>
<hr>
<table class="table	table-bordered">
	<thead>
		<th>ID</th>
		<th>Name</th>
		<th>Description</th>
		<th colspan="2" class="text-center">Action</th>
	<thead>
	<tbody>
		<% tests.forEach(function(test, i) { %>
		<tr>
			<td><%= page > 1 ? ((page-1)*10)+(i+1) : i+1 %></td>
			<td><%= test.name %></td>
			<td style="width: 400px"><%= test.description %></td>
			<td class="text-center" title="Edit"><a onclick="ajax_call('/content/edit_test/<%= test.id %>','content-admin-display')"><i class="icm icm-pencil5"></i></a></td>
			<td class="text-center" title="Delete"><a href="#"><i class="icm icm-remove"></i></a></td>
		</tr>
		<% }); %>
	</tbody>
</table>

<% if(pages > 1) { %>
	<ul class="pagination">
	  <% if(prev == true) { %>
	  	<li><a onclick='ajax_call("/test/list_tests/<%= page-1 %>","content-admin-display")'>«</a></li>
	  <% } %>
	  <% if(prev == false) { %>
	  	<li class="disabled"><a>«</a></li>
	  <% } %>
	  <% for(i = 0; i < pages ; i++) { %>
	  <li class="<%= i+1 == page ? 'active' : ''%>"><a onclick='ajax_call("/test/list_tests/<%= i+1 %>","content-admin-display")'><%= i+1 %></a></li>
	  <% } %>
	  <% if(next == true) { %>
	  	<li><a onclick='ajax_call("/test/list_tests/<%= page+1 %>","content-admin-display")'>»</a></li>
	  <% } %>
	  <% if(next == false) { %>
	  	<li class="disabled"><a>»</a></li>
	  <% } %>
	</ul>
<% } %>

