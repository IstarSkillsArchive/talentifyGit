</div>
<!-- end modal -->



<div id="node-dialog-container"></div>


<div class="oc-xs-push-push oc-sm-squeeze-push oc-md-squeeze-push oc-lg-squeeze-push modal-blur-content">

	<div id="oc-wrapper" class="oc-wrapper
       oc-lg-open-left oc-md-partial-left oc-sm-partial-left
       oc-lg-left-squeeze oc-md-left-squeeze oc-sm-left-squeeze oc-xs-left-push
       oc-lg-right-push oc-md-right-push oc-sm-right-push oc-xs-right-push
       ">

    <div class="oc-push" style="overflow-x: hidden; overflow-y: auto">
      

      <div id="main-oc-container" class="oc-container" style="margin-left: 0px">
        <nav class="navbar navbar-default navbar-static-top navbar-inplace" role="navigation">
		  <!--<ul class="nav navbar-nav navbar-left pull-left hidden-sm hidden-md hidden-lg">
		    <li>
		      <a id="oc-left-toggle-xs" href="#oc-wrapper"
		         class="btn-nav pull-left "
		         data-toggle="offcanvas" data-control="left"
		         data-states="open,hidden"
		         data-persist="lg,md,sm"
		         data-parent="body" data-sibling="right">
		        <i class="icm icm-menu6"></i>
		      </a>
		    </li>
		  </ul>-->
		<a class="navbar-brand" href="">    
		    <span class="text-gradient-success-dk"><i class="icm icm-cube icm-2x"></i>&nbsp;iLAB</span>
		</a>  
		  <ul class="nav navbar-nav navbar-right pull-right hidden-xs">
		    <li class="dropdown">
		      <a href="#" data-toggle="dropdown" class="navbar-img-link">
		        <img class="img-circle" src="/wrap/images/faker/avatars/48/Faces_Users-11.png" />
		        <span class="hidden-xs hidden-sm hidden-md"><%= currentUser.first_name %></span>
		        <b class="caret"></b>
		      </a>
		      <ul class="dropdown-menu animated fadeIn fast">
		        <li>
		<a href="">            <i class="fa fa-user"></i> Profile
		</a>        </li>
		        <li>
		<a href="">            <i class="fa fa-cog"></i> Settings
		</a>        </li>
		        <li>
		          
		<a href="/logout">              <i class="fa fa-power-off text-danger"></i>
		              <span class="text-danger">Sign out</span>
		</a>          
		        </li>
		      </ul>
		    </li>
		  </ul>
		  
		</nav>

	<!-- start container-fluid -->
    <div class="container-fluid turbo-cols-static turbo-grid-wrap gym">
		
		<div class="turbo-grid"> <!-- start turbo grid -->
		 	
		 	<ul class="nav nav-tabs">
				<li><a href="/trainer/dashboard_second/<%= currentUser.id %>"><i class="icm icm-calendar2" title="Calendar"></i></a></li>
				<li><a href="/trainer/live_class/<%= currentUser.id %>"><i class="icm icm-bubble-video-chat" title="Live Class"></i></a></li>
				<li><a href="/trainer/tot/<%= currentUser.id %>"><i class="icm icm-screen4" title="TOT"></i></a></li>
				<li class="active"><a><i class="icm icm-bubbles2" title="Alerts"></i></a></li>
				<li><a href="/trainer/billing/<%= currentUser.id %>"><i class="icm icm-paypal" title="Billing"></i></a></li>
			</ul>
			
			<div class="tab-content bg-white padded margin-bottom" style="overflow: hidden; padding-left: 0px; padding-right: 0px">
				<div class="tab-pane fade in active">
				
				<!-- start calendar -->
				<div class="col-md-12 panels">
			        <div class="panel panel-success" style="margin-bottom: 0px">
					  <div class="panel-header panel-handle panel-colorize" >
					    <ul class="nav nav-tabs">
					      <li class="active">
					        <a data-toggle="tab" href="#calendar-today">Triaged Events</a>
					      </li>
					    </ul>
					  </div>
	
					  <div class="panel-body-collapse collapse in" style="background: #eee">
					    <div class="panel-body tab-content" style="padding: 0px">
					      <div class="tab-pane active fade in padded" id="calendar-today">
						  	<ul class="nav nav-stacked nano-content" id="today-accordion">
						        <% events.forEach(function(event,i) { %>
							        <li class="chat-contact unread" style="border: 1px solid #22ba93">
									  <a class="media" style="background: #eee; padding: 15px; border: 0px">
									    <img class="media-object pull-left img-circle" src="/wrap/images/faker/avatars/48/Faces_Users-22.png">
									    <div class="media-body">
									      <div class="media-heading" style="color: #43494d;">
									        <%= event.name %>, <%= event.date.toDateString() %>, <%= event.date.getHours() %>:<%= event.date.getMinutes() %> to <%= event.endDate.getHours() %>:<%= event.endDate.getMinutes() %>, <%= event.org %>
									      </div>
									      <small class="last-message">
									        <%= event.description != null ? event.description : '' %>
									      </small>
									      <small class="last-message">
									        <btn class="btn btn-success btn-sm" onclick="non_ajax_call('/trainer/event_confirm/<%= event.id %>')">Accept</btn>
									        <btn class="btn btn-danger btn-sm" onclick="if(confirm('Are you sure?')) non_ajax_call('/trainer/event_reject/<%= event.id %>')">Reject</btn>
									      </small>
									    </div>
									  </a>
									</li>
								<% }); %>
						    </ul>
						  </div>
					      
					      
					      
					    </div>
					  </div> <!-- end panel-body-collapse -->
					</div> 
			      </div> <!-- end col-md-6 -->
				<!-- end calendar -->
				
				</div>
			</div>
		
		</div> <!-- end turbo grid -->
	
	</div>
	<!-- end container fluid -->	  
		
</div>
</div>
</div>
</div>

  
