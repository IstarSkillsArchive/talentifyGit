
<div class="animated fadeIn padded bg-info">
  	<button type="button" class="close pull-right turbo-toggle" onclick="cancelFullScreen()">&times;</button>
  	<h2 class="thin no-margin-top"><%= content.name %></h2>
	<div class="video" id="video_1" style="text-align: center; position: relative">
		<iframe src="<%= content.path %>" frameborder="0" allowfullscreen style="width: 100%; height: 480px"></iframe>
		<div style="position: absolute; bottom: 5px; right: 0px; width: 300px; height: 50px; background: transparent;">
		</div>
	</div>
	<div class="text-center">
    	<a class="btn btn-danger" onclick="toggleFullScreen()">Toggle Full Screen</a>
    	<a class="btn btn-default turbo-toggle" onclick="cancelFullScreen()">Exit</a>
		<a class="btn btn-warning" onclick="cancelFullScreen();ajax_call('/users/review_module/<%= skill_id %>','turbo-content-open-id')">Back</a>
		<input type="button" class="btn btn-primary" value="Next" data-toggle="modal" data-target="#uide4b3dde6bb5097375d99eb18a83d7146" onclick="cancelFullScreen();ajax_call('/content/show_test_instructions/<%= content.id %>/<%= skill_id %>','uide4b3dde6bb5097375d99eb18a83d7146')"/>
  	</div>
</div>



