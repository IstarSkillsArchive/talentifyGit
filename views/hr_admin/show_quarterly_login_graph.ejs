<h6 style="display: inline-block; float: left; margin-left: 10px">2014</h6>
<select style="float: right; margin: 10px; visibility: hidden" onchange="show_monthly_login_graph(this)">
	<option>Select Month</option>
	<option>January</option>
	<option>February</option>
	<option>March</option>
	<option>April</option>
	<option>May</option>
	<option>June</option>
	<option>July</option>
	<option>August</option>
	<option>September</option>
	<option>October</option>
	<option>November</option>
	<option>December</option>
</select>
<div id="chart_login" style="text-align: center">
	
</div>
<script>
	var data = [];
	<% monthly_activity.forEach(function(x, j) { %>
		data[<%= j %>] = {};
		data[<%= j %>].day = '<%= x.month %>';
		data[<%= j %>].count = <%= x.count %>;
	<% }); %>
	d3_login_display(data, 'No. of Users', 'chart_login', 'true', 'Month');
</script>

