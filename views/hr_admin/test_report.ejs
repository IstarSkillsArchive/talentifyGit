<style>
	#report-ajax .panel-body {
		height: 200px;
	}
</style>
<div class="animated fadeIn padded bg-info" id="report-ajax">
  	<button type="button" class="close pull-right turbo-toggle">&times;</button>
  	<h2 class="thin no-margin-top">Test Reports : <%= test != null ? test.name : '' %></h2>
  	
  	<div class="row">
  		<ul class="nav nav-tabs" >
		  	<li class="test_details_test_tab active" onclick="change_test_tab(this, 'test_details_test_tab')"><a onclick="ajax_call('/hr_admin/test_analytics_report/<%= test_id %>/<%= event_id %>', 'report_details')">Statistics</a></li>
		  	<li class="test_details_test_tab" onclick="change_test_tab(this, 'test_details_test_tab')"><a onclick="ajax_call('/hr_admin/test_report_card/<%= test.id %>/<%= event_id %>', 'report_details')">Report Card</a></li>
		</ul>
  		<!-- start -->
  		<div class="tab-content bg-white padded margin-bottom" style="background-color: #f2f4f8">
		  <div class="tab-pane fade in active" id="main_0">
			<div id="report_details">
				
			</div>
			
		</div>
		</div>
  		<!-- end -->
  	</div>
</div>

<script>
	ajax_call('/hr_admin/test_analytics_report/<%= test_id %>/<%= event_id %>', 'report_details');
</script>




